; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="MyBombParty"
run/main_scene="res://scenes/title.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Globals="*res://scripts/globals.gd"

[display]

window/size/viewport_width=800
window/size/viewport_height=600
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/fontawesome/plugin.cfg")

[gui]

theme/custom_font="res://fonts/NotoSansSC-Regular.ttf"

[internationalization]

locale/translations=PackedStringArray("res://i18n/template-ja.po", "res://i18n/template-zh-CN.po")
locale/translations_pot_files=PackedStringArray("res://scenes/createRoom.tscn", "res://scenes/mainGame.tscn", "res://scenes/roomList.tscn", "res://scenes/title.tscn", "res://scripts/create_room.gd", "res://scripts/GameRoom.gd", "res://scripts/GameRoomPreview.gd", "res://scripts/globals.gd", "res://scripts/main_game.gd", "res://scripts/ModernWebSocket.gd", "res://scripts/Player.gd", "res://scripts/ring_placement.gd", "res://scripts/room_item.gd", "res://scripts/room_list.gd", "res://scripts/start_button.gd", "res://scripts/user.gd")
locale/test="ja"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
